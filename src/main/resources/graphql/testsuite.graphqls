input TestSuiteInput {
    name: String!
    testCaseIds: [ID!]!
}

type TestSuite {
    id: ID!
    name: String!
    testCases: [TestCase!]!
}

extend type Query {
    findAllTestSuites: [TestSuite]!
    findTestSuite(id: ID!): TestSuite!
}

extend type Mutation {
    newTestSuite(input: TestSuiteInput!) : TestSuite!
    updateTestSuite(id: ID!, input: TestSuiteInput!) : TestSuite!
}