input TestExecutionInput {
    name: String!
    testSuiteId: ID!
    environmentId: ID!
}

type TestExecution {
    id: ID!
    name: String!
    testSuite: TestSuite!
    environment: Environment!
    testCaseExecutions: [TestCaseExecution!]!
}

extend type Query {
    findAllTestExecutions: [TestExecution]!
}

extend type Mutation {
    newTestExecution(input: TestExecutionInput!) : TestExecution!
}